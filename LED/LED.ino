
int LED=5;
void setup() {
  // put your setup code here, to run once:
  
pinMode(LED, OUTPUT);

}

void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(LED, HIGH); // turn the LED on
delay(1000); // wait for a second
//digitalWrite(LED, LOW); // turn the LED off
//delay(1000); // wait for a second

}
